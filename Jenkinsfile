pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hi') {
            steps {
                echo 'Hi'
            }
        }
         stage('test') {
            steps {
                echo 'test'
            }
        }
          stage('testing jenkinsfile') {
                    steps {
                        echo 'jenkinsfile'
                    }
                }
    }
}